@model VyatkinSchool.Models.GalleryItem
@{
    ViewBag.Title = "Изображение успешно удаленно";
}

<div id="page">
    <div class="container">
        <div class="title">
            <h2>@ViewBag.Title.</h2>
        </div>

        <h2>Удаление изображения</h2>

        <a href="@Url.Action("List", "Gallery")" class="btn btn-primary">
            Перейти к списку изображений
            <span class="glyphicon glyphicon-arrow-right" aria-hidden="true"></span>
        </a>

        <p>
            <strong>Изображение было удалено. Его востановление не возможно.</strong>
        </p>

    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

